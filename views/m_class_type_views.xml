<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_m_class_type_form" model="ir.ui.view">
            <field name="name">m.class.type.form</field>
            <field name="model">m.class.type</field>
            <field name="arch" type="xml">
                <form string="Jenis Kelas">
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="view_m_class_type_tree" model="ir.ui.view">
            <field name="name">m.class.type.tree</field>
            <field name="model">m.class.type</field>
            <field name="arch" type="xml">
                <tree string="Jenis Kelas" editable="bottom">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="action_m_class_type" model="ir.actions.act_window">
            <field name="name">Master Jenis Kelas</field>
            <field name="res_model">m.class.type</field>
            <field name="view_mode">tree,form</field>
        </record>

    </data>
</odoo>